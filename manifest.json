{
    "manifest_version": 3,
    "name": "LimpiApuntes",
    "version": "1.0.0",
    "description": "Elimina la publicidad de tus apuntes",
    "permissions": [
        "activeTab"
    ],
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "action": {
        "default_icon": {
            "16": "icon.png",
            "24": "icon.png",
            "32": "icon.png"
        },
        "default_title": "Gulag Cleaner",
        "default_popup": "popup.html"
    }
}