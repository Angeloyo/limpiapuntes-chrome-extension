{
    "manifest_version": 3,
    "name": "LimpiApuntes",
    "version": "1.0.0",
    "description": "Elimina la publicidad de tus apuntes",
    "permissions": [
        "downloads"
    ],
    "icons": {
        "16": "./img/icon.png",
        "48": "./img/icon.png",
        "128": "./img/icon.png"
    },
    "action": {
        "default_icon": {
            "16": "./img/icon.png",
            "24": "./img/icon.png",
            "32": "./img/icon.png"
        },
        "default_title": "Gulag Cleaner",
        "default_popup": "popup.html"
    }
    
}